<liferay-portlet-app>

    <portlet>
        <portlet-name>iFeedJsf</portlet-name>
        <action-url-redirect>true</action-url-redirect>
        <instanceable>false</instanceable>
        <requires-namespaced-parameters>false</requires-namespaced-parameters>
        <header-portlet-css>/css/theme.css</header-portlet-css>

        <footer-portlet-javascript>/js/se.vgregion.ifeed.Module.nocache.js</footer-portlet-javascript>
        <footer-portlet-javascript>/se.vgregion.ifeed.gwt.Module/se.vgregion.ifeed.gwt.Module.nocache.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/util.js</footer-portlet-javascript>
        <footer-portlet-javascript>/js/vgr-ifeed-config.js</footer-portlet-javascript>

        <css-class-wrapper>ifeed-portlet</css-class-wrapper>
        <add-default-resource>false</add-default-resource>
    </portlet>

