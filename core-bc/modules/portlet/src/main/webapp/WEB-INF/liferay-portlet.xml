    <portlet>
        <portlet-name>apelon-scheduler</portlet-name>
        <scheduler-entry>
            <scheduler-event-listener-class>se.vgregion.ifeed.scheduler.IFeedScheduledJob
            </scheduler-event-listener-class>
            <trigger>
                <cron>
                    <!-- Run every night at 3.15 a.m -->
                    <cron-trigger-value>0 50 13 ? * *</cron-trigger-value>
                </cron>
            </trigger>
        </scheduler-entry>
        <requires-namespaced-parameters>false</requires-namespaced-parameters>
        <system>true</system>
    </portlet>

